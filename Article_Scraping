ow nested async calls\n","nest_asyncio.apply()\n","\n","#Defines an asynchronous function scrape_article, which allows non-blocking execution\n","async def scrape_article():\n","  #Starts a context manager using async_playwright to control the browser.\n","    async with async_playwright() as p:\n","        # Starting the chromium browser invisibly\n","        browser = await p.chromium.launch(headless=True)\n","        page = await browser.new_page()\n","\n","        # Put the news article link here\n","        news_url = 'https://economictimes.indiatimes.com/news/elections/assembly-elections/jharkhand/confident-of-win-in-jharkhand-maharashtra-but-watchful-of-haryana-like-mischief-by-bjp-jairam-ramesh/articleshow/114944451.cms?from=mdr'\n","\n","        # Navigate to the news article page\n","        await page.goto(news_url)\n","\n","        # Wait for the content to load\n","        await page.wait_for_timeout(3000)  # Wait for 3 seconds\n","\n","        # Extract the content using selectors\n","        paragraphs = await page.locator('p').all_text_contents()  # Getting all text content from paragraph tags\n","\n","        # Save the content to a CSV file\n","        with open('scraped_article.csv', 'w', newline='', encoding='utf-8') as f:\n","            writer = csv.writer(f)\n","            writer.writerow(['Paragraph'])  # Adding column name\n","            for paragraph in paragraphs:\n","                writer.writerow([paragraph])\n","\n","        # Print the number of paragraphs scraped\n","        print(f'Scraped {len(paragraphs)} paragraphs')\n","\n","        # Close the browser\n","        await browser.close()\n","\n","# Run the async function\n","asyncio.run(scrape_article())\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"q7URUrkfV7Bp","executionInfo":{"status":"ok","timestamp":1730726205614,"user_tz":-330,"elapsed":31957,"user":{"displayName":"Aneeka.Jain Btech2023","userId":"16829025308143759947"}},"outputId":"12a9cd15-131a-4885-f926-97a7599be00f"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Scraped 107 paragraphs\n"]}]},{"cell_type":"markdown","source":["https://www.livemint.com/topic/maharashtra-election"],"metadata":{"id":"lCKZYTHQbJNa"}}]}